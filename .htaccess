<IfModule mod_expires.c>
ExpiresActive On
ExpiresDefault "access 1 month"
</IfModule>
RewriteEngine On
RewriteCond %{HTTPS} !=on
RewriteRule ^ https://%{HTTP_HOST}%{REQUEST_URI} [L,R=301]
ErrorDocument 404 /
Redirect 301 /2 http://bocracy.tk
Redirect 301 /index.html /b
Redirect 301 /ios https://appsto.re/us/OUwGib.i
Redirect 301 /android https://goo.gl/j7DaH6
Redirect 301 /chrome https://goo.gl/e5FmiS
Redirect 301 /betatest https://groups.google.com/d/forum/b-ocracy-beta-testers
Redirect 301 /support mailto:team@bocracy.com